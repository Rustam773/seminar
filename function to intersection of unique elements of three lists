def intersection_of_three_lists(list1, list2, list3):
   
    set1, set2, set3 = set(list1), set(list2), set(list3)
    
    
    intersection = list(set1 & set2 & set3)
    
    return intersection

list1 = [1, 2, 2, 3, 4]
list2 = [2, 3, 5, 3]
list3 = [3, 2, 6, 2]

result = intersection_of_three_lists(list1, list2, list3)
print(result)
